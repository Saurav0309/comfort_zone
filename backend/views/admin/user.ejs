<!DOCTYPE html>
<html>
<head>
  <title>Manage Users</title>
</head>
<body>
  <h1>Users</h1>
  <table>
    <tr>
      <th>Email</th>
      <th>Action</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.email %></td>
        <td>
          <form action="/admin/users/delete/<%= user._id %>" method="POST">
            <button type="submit">Delete</button>
          </form>
        </td>
      </tr>
    <% }) %>
  </table>
</body>
</html>